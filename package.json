{
  "name": "ssr-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-client": "webpack --config webpack.client.js",
    "watch-client": "set DEVELOPMENT=true && webpack --config webpack.client.js --watch",
    "analyze": "set ANALYZE=true && npm run build-client",
    "build-server": "webpack --config webpack.server.js",
    "watch-server": "set DEVELOPMENT=true && webpack --config webpack.server.js --watch",
    "build": "npm run build-client && npm run build-server",
    "watch": "concurrently \"npm run watch-client\" \"npm run watch-server\" \"npm run start-dev\"",
    "start": "node build/bundle.js",
    "start-dev": "nodemon build/bundle.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "device": "^0.3.11",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^6.1.0",
    "serialize-javascript": "^5.0.1",
    "dotenv": "^8.2.0",
    "webpack-node-externals": "^1.7.2",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "compression-webpack-plugin": "^6.0.2",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
